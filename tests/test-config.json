{
	"profile": "dev-profile.json",
	"chromeConfig":{
		"flags": "--window-size=1280,720"
	},
	"browserConfig": {
		"viewport": {
			"width": 1680,
			"height": 1280
		},
		"timeout": 60000,
		"headless": true,
		"dumpio": false,
		"closeBrowser": {
			"onFailure": false,
			"onSuccess": false
		},
		"devtools": true,
		"closeTab": {
			"onFailure": false,
			"onSuccess": false
		}
	},
	"globalInject": [
		"tests/google.js",
		"tests/pdf-test.js",
		"tests/expectations/pdf-expect.js"
	],
	"testSuite": {
		"suite1": {
			"tab1": [
				"${google.com}.testGoogleSearch",
				"${google.com}.testGoogleSearch"
			],
			"tab2": [
				"${graduateland.com}.testPdfTextContent",
				"${graduateland.com}.testPdfTextContent"
			]
		},

		"suite2": {
			"tab1": [
				"${google.com}.testGoogleSearch"
			],
			"tab2": [
				"${graduateland.com}.testPdfTextContent"
			]
		},

		"devSuite": {
			"tab1": [
				"${google.com}.testGoogleSearch"
			]
		}
	}
}